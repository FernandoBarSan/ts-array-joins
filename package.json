{
    "name": "ts-array-joins",
    "version": "1.0.2",
    "description": "Strongly-typed TypeScript utilities for array grouping, SQL-like joins, and data aggregation. Zero dependencies, full type inference, O(n+m) performance.",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "lint": "tsc --noEmit && vitest run",
        "prepublishOnly": "npm run build && npm test"
    },
    "keywords": [
        "typescript",
        "array",
        "grouping",
        "group-by",
        "join",
        "sql-join",
        "inner-join",
        "left-join",
        "one-to-many",
        "one-to-one",
        "data-manipulation",
        "array-operations",
        "lodash-alternative",
        "ramda-alternative",
        "composite-keys",
        "multi-key",
        "aggregation",
        "data-transformation",
        "type-safe",
        "strongly-typed",
        "zero-dependencies",
        "functional",
        "immutable",
        "esm",
        "commonjs",
        "prisma",
        "orm",
        "database",
        "json",
        "data-processing"
    ],
    "author": "Fernando Barr√≥n <fernando_barsan@hotmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FernandoBarSan/ts-array-joins.git"
    },
    "bugs": {
        "url": "https://github.com/FernandoBarSan/ts-array-joins/issues"
    },
    "homepage": "https://github.com/FernandoBarSan/ts-array-joins#readme",
    "devDependencies": {
        "@types/node": "^20.11.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
