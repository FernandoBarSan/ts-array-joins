{
    "name": "ts-array-joins",
    "version": "1.0.0",
    "description": "Strongly-typed TypeScript utilities for array grouping and SQL-like joins",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "test": "vitest run",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit",
        "lint": "tsc --noEmit && vitest run",
        "prepublishOnly": "npm run build && npm test"
    },
    "keywords": [
        "typescript",
        "array",
        "group",
        "join",
        "sql",
        "groupby",
        "utilities",
        "composite-keys"
    ],
    "author": "Fernando Barr√≥n <fernando_barsan@hotmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FernandoBarSan/ts-array-joins.git"
    },
    "bugs": {
        "url": "https://github.com/FernandoBarSan/ts-array-joins/issues"
    },
    "homepage": "https://github.com/FernandoBarSan/ts-array-joins#readme",
    "devDependencies": {
        "@types/node": "^20.11.0",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "vitest": "^1.2.0"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}